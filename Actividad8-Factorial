#Factorial con Iteracion
num = int(input('Ingresa numero para factorial: '))
for cont in range(1,num):
    num *= cont
    
print (num)



#Factorial con Recursividad
def factorial(num):
    if num == 0:
        return 1
    else:
        return num * factorial(num-1)

print(factorial(int(input('Ingresa numero para factorial: '))))
